<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>企业定位</title>
    <link rel="stylesheet" href="~/Content/themes/showview/Site.css" type="text/css" />
    <style>
        .label
        {
            width: 40%;
        }
        .post
        {
            width: 100%;
            float: left;
            clear: both;
            border-bottom: 1px #1a2246 dotted;
        }
        .valuetxt
        {
            text-align: left;
        }
        input
        {
            width: 100%;
        }
    </style>
</head>
<body>
    <div>
        <div class="div100 title yellow"><a href="javascript:" style="float:left"  onclick="javascript:history.go(-1);"> <span class="btnReturn"> 返回 </span></a>企业定位/button></div>
        <form action="SaveTdxzInfo" method="post">
            <div id="ltdpos">
                <div class="post">
                    <div class="div100 yellow">企业名称:</div>
                    <div class="div100">
                        <div>
                            <div style="width: 100%; float: left;"><input  value="苏"/></div>
                        </div>
                    </div>
                    <div style="text-align: center; margin-top: 5px; clear: both;">
                        <input id="postChange" style="width:25%" type="button" onclick="ViewToEdit($('#postChange').val())" value="定位"/>
                    </div>
                </div>
            </div>
        </form>
        <div>
            <div class="div100">
                <div>
                    <div style="width: 0%; float: left; visibility:hidden" >&nbsp;&nbsp;地块编码：{item.地块编码}</div>
                    <div style="width: 100%; float: left;"><a href="#" onclick="定位">&nbsp;&nbsp;用地单位名称：{item.用地单位名称}</a></div>
                </div>
            </div>
         </div>
    </div>
    <script type="text/javascript" src="~/Content/themes/showview/vue.js"></script>
    <script type="text/javascript" src="~/Content/themes/showview/zepto.min.js"></script>
       <script tyep="text.javascript"  src="~/Scripts/QyTech.js"></script>
 
    <script>
        Id = "@ViewBag.Id";
        var vm = new Vue({
            el: '#tdxz',
            data: {
                item: [],
            },
        });
        console.log(123)
        $.post('../EditView/GetTdxzInfo', { id: Id }, function (data) {
            console.log(data);
            if (data != null) {
                vm.item = data;
            } else {
                alert("暂无数据！");
            }
        });

 
    </script>
</body>
</html>
